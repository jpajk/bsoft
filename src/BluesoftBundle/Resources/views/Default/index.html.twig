{% extends 'BluesoftBundle::template.html.twig' %}
{% block body %}
    <div class="row">
        <div class="form-wrapper">
            {{ form_start(form) }}
            <div class="uploader">
                {{ form_widget(form.spreadsheet, { 'attr': {'class': 'form-control  '} }) }}
                {{ form_widget(form.submit, { 'attr': {'class': 'btn btn-primary'} }) }}
            </div>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}